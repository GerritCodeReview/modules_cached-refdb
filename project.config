[access]
	inheritFrom = Public-Projects
[submit]
	action = inherit
[access "refs/*"]
	owner = group modules-cached-refdb
	label-Verified = -1..+1 group gerrit-verifiers
	create = group modules-cached-refdb
	push = +force group modules-cached-refdb
[project]
	description = Gerrit libModule to cache access to Git repositories' refdb
